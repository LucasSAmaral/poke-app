{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "NjcyMmViOTMtMTQ1YS00NDEzLTgxM2QtZTA0MjViNTI5ODQzfHJlYWQtd3JpdGU="
      }
    }
